{
  "name": "@verify/server",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm run build && node dist/app",
    "dev:sudo": "npm run build && sudo DEBUG=utils:createAgentRouter NODE_TLS_REJECT_UNAUTHORIZED=\"0\" node dist/app",
    "clean": "rimraf dist",
    "build": "yarn clean && tsc",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:prod": "node dist/app",
    "test:veramo": "jest --forceExit --detectOpenHandles veramo.unit-test",
    "test:issuer": "jest --forceExit --detectOpenHandles issuer.unit-test",
    "test:tenant": "jest --forceExit --detectOpenHandles tenant.unit-test"
  },
  "dependencies": {
    "@veramo/core": "^1.2.0",
    "@veramo/credential-w3c": "^1.2.0",
    "@veramo/data-store": "^1.2.0",
    "@veramo/did-jwt": "^1.2.0",
    "@veramo/did-manager": "^1.2.0",
    "@veramo/did-provider-key": "^1.2.0",
    "@veramo/did-provider-web": "^1.2.0",
    "@veramo/did-resolver": "^1.2.0",
    "@veramo/key-manager": "^1.2.0",
    "@veramo/kms-local": "^1.2.0",
    "@veramo/message-handler": "^1.2.0",
    "@veramo/remote-client": "^1.2.0",
    "@veramo/remote-server": "^1.2.0",
    "@veramo/did-comm": "^1.2.0",
    "@veramo/selective-disclosure": "^1.2.0",
    "web-did-resolver": "^2.0.4",
    "blakejs": "^1.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.5.0",
    "lodash": "^4.17.21",
    "jose2": "npm:jose@^2.0.4",
    "jwt-decode": "^3.1.2",
    "oidc-provider": "^7.5.1",
    "pg": "^8.6.0",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.32",
    "vhost": "^3.0.2",
    "yamljs": "^0.3.0",
    "yup": "^0.32.9"
